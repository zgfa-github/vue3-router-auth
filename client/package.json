{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "commit": "git-cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "vue": "^3.2.37",
    "vue-router": "^4.1.5",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@types/node": "^18.7.23",
    "@types/qs": "^6.9.7",
    "@types/vue-router": "^2.0.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "commitizen": "4.2.4",
    "cz-customizable": "6.3.0",
    "husky": "7.0.1",
    "mockjs": "^1.1.0",
    "qs": "^6.11.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-plugin-mock": "^2.9.6",
    "vue-tsc": "^0.40.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "cz-customizable": {
    "config": "cz-config.js"
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "eslint --fix",
      "git add ."
    ]
  }
}
